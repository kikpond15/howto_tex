  \Mul\abs@vy\@yscale\abs@vy
  \Abs\abs@vx\abs@vx
  \Abs\abs@vy\abs@vy
  \ifdim\abs@vx\p@<\abs@vy\p@
    \edef\abs@max{\abs@vy}\edef\abs@min{\abs@vx}%
  \else
