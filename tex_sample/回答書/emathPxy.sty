  \else
    \Div\abs@min\abs@max\abs@a
    \Mul\abs@a\abs@a\abs@a
    \Add\abs@a{1}\abs@a
    \Sqroot\abs@a\abs@a
    \Mul\abs@a\abs@max#2\relax
